#include <stdio.h>
struct point { int x, y; };

void bucket_sort(struct point a[], int n) {
  struct point b[128];
  int m, i;

  int c[100] = {};
  for(m=0; m<n; m++){
    c[a[m].x]++;
    b[m] = a[m];
  }
  for(m=1; m<100; m++){
    c[m] = c[m] + c[m-1];
  }
  for(m=n-1; m>=0; m--){
    i = b[m].x;
    c[i]--;
    a[c[i]] = b[m];
  }
}

int main(){
  char buf[128];
  struct point p, arr[128];
  int i = 0, n;
  while(fgets(buf,sizeof(buf),stdin)!=NULL && i<128) {
    sscanf(buf,"%d %d",&p.x, &p.y);
    arr[i] = p;
    ++i;
  }
  n = i;
  bucket_sort(arr, n);
  for(i=0;i<n;++i)
    printf("%dã€€%d\n", arr[i].x, arr[i].y);
  return 0;
}
